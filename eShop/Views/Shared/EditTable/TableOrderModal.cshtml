@model List<eShop.Models.Order>
<div class="table table-responsive">
    <table class="table table-hover">
        <thead>
            <tr>
                <th>id</th>
                <th>Soyad</th>
                <th>Ad</th>
                <th>Telefon</th>
                <th>Email</th>
                <th>Status</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var or in Model)
            {
                <tr>
                    <td>@or.id</td>
                    <td>@or.LastName</td>
                    <td>@or.FirstName</td>
                    <td>@or.Phone</td>
                    <td>@or.Email</td>
                    <td>@or.status</td>
            </tr>
            }
        </tbody>
    </table>
</div>